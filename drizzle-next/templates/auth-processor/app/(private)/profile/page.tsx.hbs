import { auth } from "@/lib/auth";
import {
  Dashboard,
  DashboardContent,
  DashboardTitle,
} from "@/components/ui/dashboard";

export default async function Page() {
  const session = await auth();
  return (
    <Dashboard>
      <DashboardTitle>Profile</DashboardTitle>
      <DashboardContent>
        <strong>Email:</strong> {session?.user?.email}
      </DashboardContent>
    </Dashboard>
  );
}
