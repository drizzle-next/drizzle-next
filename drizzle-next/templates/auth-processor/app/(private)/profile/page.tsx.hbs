import { auth } from "@/lib/auth";
import {
  PageLayout,
  PageContent,
  PageTitle,
} from "@/components/ui/page-layout";

export default async function Page() {
  const session = await auth();
  return (
    <PageLayout>
      <PageTitle>Profile</PageTitle>
      <PageContent>
        <strong>Email:</strong> {session?.user?.email}
      </PageContent>
    </PageLayout>
  );
}
